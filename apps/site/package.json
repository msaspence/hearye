{
  "name": "@hearye/site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build": "npm run build:static",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:static": "BUILD_FAVICONS=true vite build --outDir dist/static && npm run build:server && node prerender",
    "serve": "NODE_ENV=production node server",
    "start": "serve dist/static",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "prando": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "serve": "^14.0.1",
    "serve-static": "^1.15.0"
  }
}
